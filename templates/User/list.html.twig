{% extends 'generic_layout.html.twig' %}

{% block head_title %}Liste - Membres{% endblock %}
{% block main_title %}Liste de nos membres{% endblock %}

{% block main_content %}
<table>
	<thead>
		<th>Pseudo</th>
		<th>Action</th>
	</thead>
	<tbody>
		{% for entity in entities %}
			<tr>
				<td>{{ entity.username }}</td>
				<td><a href="{{ path('user_show', {'id' : entity.id }) }}">Lire</a></td>
			</tr>
		{% endfor %}
	</tbody>
</table>
{% endblock %}